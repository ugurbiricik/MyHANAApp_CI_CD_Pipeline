<mvc:View
	controllerName="com.abics.ugur.controller.Main"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	>

	<Page title="Overview Page">
	<Table
  id="itemsTable"
  items="{/Interactions_Items}">
  <columns>
    <Column>
      <Text text="Text" />
    </Column>
    <Column>
      <Text text="Date" />
    </Column>
    <Column>
      <Text text="Price" />
    </Column>
  </columns>
  <items>
    <ColumnListItem>
      <cells>
        <Text text="{text}" />
        <Text text="{date}" />
        <Text text="{price}" />
      </cells>
    </ColumnListItem>
  </items>
</Table>


	</Page>

</mvc:View>
