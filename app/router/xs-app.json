{
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "srv-api",
      "csrfProtection": true
    },
    {
      "source": "^(.*)$",
      "target": "/$1",
      "localDir": "webapp",
      "cacheControl": "no-cache, no-store, must-revalidate"
    }
  ]
}
